{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@pages/*": ["src/pages/*"],
      "@assets/*": ["src/assets/*"],
      "@components/*": ["src/components/*"],
      "@features/*": ["src/features/*"],
      "@commons/*": ["src/commons/*"],
      "@redux/*": ["src/redux/*"],
      "@templates/*": ["src/templates/*"],
      "@pdf/*": ["src/pdf/*"],

      "@report-blocks/*": ["src/pdf/blocks/*"],
      "@report-images/*": ["src/pdf/images/*"],
      "@report-layout/*": ["src/pdf/layout/*"]
    }
  }
}